{
  "name": "pogo/scheduler",
  "description": "Pogo Native Scheduler for Laravel",
  "keywords": [
    "laravel",
    "frankenphp",
    "pogo",
    "scheduler",
    "cron"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.5",
    "laravel/framework": "^12.44.0",
    "laravel/octane": "^2.13.3",
    "friendsofphp/php-cs-fixer": "^3.92.3"
  },
  "autoload": {
    "psr-4": {
      "Pogo\\Scheduler\\": "lib/src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pogo\\Scheduler\\SchedulerServiceProvider"
      ]
    }
  }
}
